

package SubjectHeadings.IfFerhat;

public class ifControl {
    public static void main(String[] args) {
        int number1=45;
        int number2=35;

        if (number1>number2){

            System.out.println("number1 greates than number2");

        }
        if (number1<number2){
            System.out.println("number1less than number2");
        }
        if (number1 !=number2){
            System.out.println("number1 dosen't equal number2");

        }
        if (number1==number2){
            System.out.println("number1 equal number2");
        }
        System.out.println("comparison finished");

    }



}



